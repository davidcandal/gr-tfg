  <callback>setAPS_frameType($APS_frameType)</callback>
  <callback>setAPS_deliveryMode($APS_deliveryMode)</callback>
  <callback>setAPS_ackFormat($APS_ackFormat)</callback>
  <callback>setAPS_security($APS_security)</callback>
  <callback>setAPS_ackRequest($APS_ackRequest)</callback>
  <callback>setAPS_extHeaderPresent($APS_extHeaderPresent)</callback>
  <callback>setAPS_destinationEndpoint($APS_destinationEndpoint)</callback>
