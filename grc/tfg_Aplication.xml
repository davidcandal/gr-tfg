  <callback>setAPS_blockNumber($APS_blockNumber)</callback>
  <callback>setAPS_ackBitfield($APS_ackBitfield)</callback>
  <callback>setAPS_commandID($APS_commandID)</callback>
  
  <sink>
    <name>in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
  
  <source>
    <name>out</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
