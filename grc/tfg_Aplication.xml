<?xml version="1.0"?>
<block>
  <name>Aplication</name>
  <key>tfg_Aplication</key>
  <category>[TFG]</category>
  <import>import tfg</import>
  <make>tfg.Aplication()</make>

  <callback>send($send)</callback>
  <callback>setPayload($payload)</callback>
  <callback>toggleRedLed($led)</callback>
  <callback>toggleGreenLed($led)</callback>
