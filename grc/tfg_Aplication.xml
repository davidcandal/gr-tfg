  <callback>setMAC_intraPAN($MAC_intraPAN)</callback>
  <callback>setMAC_destMode($MAC_destMode)</callback>
  <callback>setMAC_srcMode($MAC_srcMode)</callback>
  <callback>setMAC_destPAN($MAC_destPAN)</callback>
  <callback>setMAC_destAddress($MAC_destAddress)</callback>
  <callback>setMAC_srcPAN($MAC_srcPAN)</callback>
  <callback>setMAC_srcAddress($MAC_srcAddress)</callback>

  <callback>setNWK_frameType($NWK_frameType)</callback>
  <callback>setNWK_discoverRoute($NWK_discoverRoute)</callback>
  <callback>setNWK_multicastFlag($NWK_multicastFlag)</callback>
  <callback>setNWK_security($NWK_security)</callback>
  <callback>setNWK_sourceRoute($NWK_sourceRoute)</callback>
  <callback>setNWK_hasDestIEEE($NWK_hasDestIEEE)</callback>
  <callback>setNWK_hasSrcIEEE($NWK_hasSrcIEEE)</callback>
  <callback>setNWK_destinationAdd($NWK_destinationAdd)</callback>
  <callback>setNWK_sourceAdd($NWK_sourceAdd)</callback>
  <callback>setNWK_radius($NWK_radius)</callback>
  <callback>setNWK_destinationIEEE($NWK_destinationIEEE)</callback>
  <callback>setNWK_sourceIEEE($NWK_sourceIEEE)</callback>
