<?xml version="1.0"?>
<block>
    <name>IEEE802.15.4 MAC modified</name>
    <key>tfg_mac</key>
    <category>[TFG]</category>
    <import>import tfg</import>
    <make>tfg.mac($debug)</make>

    <param>
        <name>Debug</name>
        <key>debug</key>
        <value>False</value>
        <type>bool</type>

        <option>
            <name>Enable</name>
            <key>True</key>
        </option>
        <option>
            <name>Disable</name>
            <key>False</key>
        </option>
    </param>
    
    <sink>
        <name>pdu in</name>
        <type>message</type>
        <optional>1</optional>
    </sink>
    <sink>
        <name>app in</name>
        <type>message</type>
        <optional>1</optional>
    </sink>
    <source>
        <name>pdu out</name>
        <type>message</type>
        <optional>1</optional>
    </source>
    <source>
        <name>app out</name>
        <type>message</type>
        <optional>1</optional>
    </source>

</block>
